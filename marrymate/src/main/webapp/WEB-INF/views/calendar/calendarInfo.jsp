  <script>
   (function(SIM10, $, undefined) {

	'use strict';

	function initialize() {
		if (0 < $("div[class^=mainTopBanner]").length) {
			openPopup();
		}
	}
	
	function setCookie(name, value, expire) {
		var todayValue = new Date();

		todayValue.setDate(todayValue.getDate() + expire);
		document.cookie = name + "=" + encodeURI(value) + "; expires=" + todayValue.toGMTString() + "; path=/;";
	}
	
	function getCookie(name) { 
		var cookieName = name + "=";
		var index = 0;
		var cookie = "";
		
		while (index <= document.cookie.length) {
			var key = (index + cookieName.length);
			
			if (document.cookie.substring(index, key) == cookieName) {
				if ((cookie=document.cookie.indexOf(";", key)) == -1) {
					cookie = document.cookie.length;
				}
				
				return decodeURI(document.cookie.substring(key, cookie));
			}
		
			index = document.cookie.indexOf(" ", index) + 1;
			
			if (index ==0) break;
		}
		
		return "";
	}
	
	function openPopup() {
		if ("" == SIM10.getCookie("topPopup" + SIM10.getTopPopupId())) {
			if($(".mainTopBanner").length > 0){
				$(".mainTopBanner").slideDown(500);
				$("body").addClass("mainTopBannerType");
			}
		}
		
	}
	
	function getTopPopupId() {
		var topPopupId = "";
		
		if ($(".mainTopBanner").length > 0) {
			for (var i = 0; i < $(".mainTopBanner .inner .swiper-slide").length; i++) {
				if ("block" == $(".mainTopBanner .inner .swiper-slide").eq(i).css("display")) {
					topPopupId = $(".mainTopBanner .inner .swiper-slide").eq(i).find("img").data("nm");
				}
			}
		}
		
		return topPopupId;
	}
	
	function goToUrl() {
		
	}
	// 익스포트
	SIM10.initialize = initialize;
	SIM10.setCookie = setCookie;
	SIM10.getCookie = getCookie;
	SIM10.getTopPopupId = getTopPopupId;
	SIM10.openPopup = openPopup;
	SIM10.goToUrl = goToUrl;
	
})(window.SIM10 = window.SIM10 || {}, jQuery);
  </script>
